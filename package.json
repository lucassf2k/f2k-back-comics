  "description": "An API rest to distribute comics to both readers and creators",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc --project tsconfig-build.json && tsc-alias",
    "start": "node .",
    "start:dev": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register src/main.ts' -e ts",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:watch": "pnpm test -- --watch",
    "test:coverage": "pnpm test -- --coverage"
  },
  "keywords": [],
